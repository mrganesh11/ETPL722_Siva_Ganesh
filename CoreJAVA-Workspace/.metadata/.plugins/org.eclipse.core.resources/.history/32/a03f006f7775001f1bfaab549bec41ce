import com.webobjects.appserver.WOComponent;
import com.webobjects.appserver.WOContext;
import com.webobjects.foundation.NSMutableArray;

public class Main extends WOComponent {
	private static final long serialVersionUID = 1L;
	String myName;
	String address;
	boolean flag;
	
	public boolean isMale;
	public boolean isFemale;
	public String gender;
	boolean isTelugu;
	boolean isEnglish;
	boolean isHindi;
	String itr;
	protected NSMutableArray bglist=new NSMutableArray();
	protected NSMutableArray KL=new NSMutableArray();
	public Main(WOContext context) {
		super(context);
		bglist.addObject("A");
		bglist.addObject("B");
		bglist.addObject("AB");
		bglist.addObject("O");
		
	}
	public WOComponent submit() {
		System.out.println("submitted successfully");
		flag=true;
		if(isMale)
			gender="Male";
		if(isFemale)
			gender="Female";
		
		if(isTelugu) KL.addObject("Telugu"); 
		if(isEnglish) KL.addObject("English");
		if(isHindi) KL.addObject("Hindi");

		return null;
	}
	public WOComponent back()
	{
		System.out.println("Back successfully");
		flag=false;
		myName="";
		address="";
		return null;
	}
}